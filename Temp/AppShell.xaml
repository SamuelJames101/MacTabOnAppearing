<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Temp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Temp"
    Shell.FlyoutBehavior="Flyout"
    Title="Temp">

<TabBar>
    <Tab Title="1">
        <ShellContent Route="1" ContentTemplate="{DataTemplate local:MainPage}"/>
    </Tab>
    <Tab Title="2">
        <ShellContent Route="2" ContentTemplate="{DataTemplate local:SecondPage}"/>
    </Tab>
</TabBar>

</Shell>
